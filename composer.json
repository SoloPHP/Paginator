{
  "name": "solophp/paginator",
  "type": "library",
  "description": "PHP pagination package",
  "keywords": ["php", "paginator", "pagination", "psr-7"],
  "license": "MIT",
  "authors": [
    {
      "name": "Vitaliy Olos",
      "email": "vedastudio@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Solo\\Paginator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Solo\\Paginator\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs --standard=PSR12 src/ tests/",
    "cs-fix": "phpcbf --standard=PSR12 src/ tests/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}